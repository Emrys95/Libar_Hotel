    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="index.html"></a>

            <img src="../../Content/images/Libar_logo.png" height="120" width="120" />

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Meni
            </button>
            <div class="collapse navbar-collapse" id="ftco-nav">
               
                <ul class="navbar-nav ml-auto">


                    <li class="nav-item active"><a href="#" class="nav-link">Početna</a></li>
                    <li class="nav-item"><a href="#rooms" class="nav-link">Sobe</a></li>
                    <li class="nav-item"><a href="#restaurants" class="nav-link">Restorani</a></li>
                    <li class="nav-item"><a href="#about_us" class="nav-link">O nama</a></li>
                    <li class="nav-item"><a href="#contact" class="nav-link">Kontakt</a></li>
                    <li class="nav-item">
                        <a href="@Url.Action("Register", "Account")" class="nav-link"><span class="glyphicon glyphicon-user"></span> Registruj se</a>
                    </li>

                    @if (User.Identity.IsAuthenticated)
                    {

                        <li style="display:none" class="nav-item"> <a href="@Url.Action("Login", "Account")" class="nav-link"><span class="glyphicon glyphicon-log-in"></span> Uloguj se</a></li>

                    }
                    else
                    {
                        <li class="nav-item"> <a href="@Url.Action("Login", "Account")" class="nav-link"><span class="glyphicon glyphicon-log-in"></span> Ulogujte se</a></li>
                    }
                    @if (User.IsInRole("Gost"))
                    {
                        using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                        {
                            @Html.AntiForgeryToken()


                            <li class="nav-item"><a href="@Url.Action("Manage", "User", routeValues: new { userId = User.Identity.GetUserId() })" class="nav-link">Zdravo @User.Identity.Name!</a></li>
                            <li class="nav-item"><a href="javascript:document.getElementById('logoutForm').submit()" class="nav-link">Izloguj se</a></li>


                        }
                    }
                </ul>
                
            </div>
        </div>
